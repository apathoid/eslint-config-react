{
    "name": "@apathoid/eslint-config-react",
    "version": "1.0.1",
    "description": "React ESLint configuration file.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apathoid/eslint-config-react.git"
    },
    "type": "module",
    "main": "./index.cjs",
    "exports": {
        "import": "./index.js",
        "require": "./index.cjs",
        "default": "./index.cjs"
    },
    "files": [
        "index.js",
        "index.cjs",
        "index.d.ts"
    ],
    "scripts": {
        "build:cjs": "babel index.js -o index.cjs",
        "prepare": "npm run build:cjs"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "linter"
    ],
    "author": "apathoid",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/apathoid/eslint-config-react/issues"
    },
    "homepage": "https://github.com/apathoid/eslint-config-react#readme",
    "peerDependencies": {
        "eslint": ">=8",
        "eslint-plugin-react": ">=7",
        "eslint-plugin-react-hooks": ">=4"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.23.7",
        "@babel/plugin-transform-modules-commonjs": "^7.23.3"
    }
}
